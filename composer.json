{
  "name": "msbios/debug",
  "description": "MSBios Debug",
  "license": "MIT",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-debug",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Debug\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Debug\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-developer-tools": "^1.1",
    "zendframework/zend-debug": "^2.5",
    "zfcampus/zf-development-mode": "^3.0",
    "san/san-session-toolbar": "^1.0",
    "digitalnature/php-ref": "^1.2",
    "matycz/lemo-tracy": "^1.0",
    "ccampbell/chromephp": "^4.1",
    "msbios/msbios": "^1.0"
  },
  "require-dev": {
    "msbios/test": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  }
}
